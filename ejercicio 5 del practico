void Solucion1(float O, float P, float Q, float R, float S, float T, float &M, float &N)
{ M = ((O * T) - (R * Q)) / ((O * S) - (R * P));
  N = (Q - P * N)/ O;
  Q=7;
  cout<<"\n El valor de X es igual a "<< M;
  cout<<"\n El valor de Y es igual a "<< N;
}
